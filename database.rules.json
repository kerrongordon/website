{
  "rules": {
    "portfolios": {
      ".read": true,
      ".write": "root.child('users/' + auth.uid + '/isAdmin').val() === true"
    },
    "skills": {
      ".read": true,
      ".write": "root.child('users/' + auth.uid + '/isAdmin').val() === true"
    },
    "users": {
      "$uid": {
        ".read": true,
        ".write": "auth.uid !== null"
      }
    }
  }
}